<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/layout_10dp"
    android:orientation="vertical">

    <LinearLayout
        style="@style/itemstyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_seq_no"
            android:layout_width="@dimen/layout_60dp"
            android:layout_height="wrap_content"
            android:text="@string/seq_no"
            android:textColor="@color/black32"
            android:textSize="@dimen/textSize_14sp" />

        <EditText
            android:id="@+id/et_seq_no"
            android:layout_width="@dimen/layout_0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@null"
            android:singleLine="true"
            android:inputType="number"
            android:layout_marginLeft="@dimen/layout_10dp"
            android:textColor="@color/black_32"
            android:textCursorDrawable="@drawable/et_cursor_yellow"
            android:textSize="@dimen/textSize_14sp" />
    </LinearLayout>

    <View
        android:id="@+id/line_seq_no"
        android:layout_width="match_parent"
        android:layout_height="@dimen/layout_0.5dp"
        android:background="@color/BG_ALL"></View>
    <LinearLayout
        style="@style/itemstyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_check_value"
            android:layout_width="@dimen/layout_60dp"
            android:layout_height="wrap_content"
            android:text="@string/check_value"
            android:textColor="@color/black32"
            android:textSize="@dimen/textSize_14sp" />

        <EditText
            android:id="@+id/et_check_value"
            android:layout_width="@dimen/layout_0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@null"
            android:singleLine="true"
            android:inputType="number"
            android:layout_marginLeft="@dimen/layout_10dp"
            android:textColor="@color/black_32"
            android:textCursorDrawable="@drawable/et_cursor_yellow"
            android:textSize="@dimen/textSize_14sp" />
    </LinearLayout>

    <View
        android:id="@+id/line_check_value"
        android:layout_width="match_parent"
        android:layout_height="@dimen/layout_0.5dp"
        android:background="@color/BG_ALL"></View>
    <LinearLayout
        style="@style/itemstyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_isOK"
            android:layout_width="@dimen/layout_0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="@string/isOK"
            android:textColor="@color/black32"
            android:textSize="@dimen/textSize_14sp" />

        <CheckBox
            android:id="@+id/cb_isOK"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/layout_25dp"
            android:button="?attr/checkvalue_checkbox" />
    </LinearLayout>

    <View
        android:id="@+id/line_isOK"
        android:layout_width="match_parent"
        android:layout_height="@dimen/layout_0.5dp"
        android:background="@color/BG_ALL"></View>

    <include layout="@layout/inclu_save"></include>

</LinearLayout>